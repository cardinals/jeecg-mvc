#配置
eserver.home = 
kafka.url = 192.168.10.10:9091,192.168.10.11:9092,192.168.10.12:9093
zookeeper.url = 192.168.1.41:2182,192.168.1.42:2182,192.168.1.235:2182
#server.port.start=5001
server.port.count=50
#ip.port=4998
#超时设置 毫秒
timeout=10000
# 本地IP
local.ip=192.168.1.29
# 心跳是否开启，true 开启，false 不开启，默认开启
beatting.open=false
# 心跳发送周期，单位秒
#heart.beating.time = 1
# 数据验证方式（md5,rsa）
#scurity.type = md5
# md5加盐
#md5.salt = emotte
# 扫描器扫描类文件
scan.regex = com.emotte
# 内容路径，三种格式classpath:context.xml, 全路径格式, ./context.xml(表示从home路径的conf文件夹中查找)
context = classpath:context.xml